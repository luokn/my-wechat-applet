.container {
    display: flex;
    flex-direction: column;
    margin: 20rpx;
}

.user {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-top: 120rpx;
}

.user text {
    margin-top: 20rpx;
}

.mid {
    display: flex;
    flex-direction: column;

    padding: 20rpx;
    margin-top: 20rpx;
    border-radius: 20rpx;
    background-color: white;
}

.panel {
    display: flex;
    flex-direction: column;

    margin-top: 20rpx;
    border-radius: 20rpx;
    background-color: white;
}

.head {
    padding: 25rpx 20rpx 25rpx 20rpx;
    border-bottom: 1rpx solid #f1f1f1;
}

.items {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: space-between;

    padding: 20rpx;
}

.item {
    display: flex;
    flex-direction: column;
    align-items: center;

    margin: 30rpx 0rpx 30rpx 0rpx;
    width: 30%;
}

.item text {
    margin-top: 10rpx;
}
